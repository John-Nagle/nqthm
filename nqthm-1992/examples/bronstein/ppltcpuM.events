(DEFN TOPOR-SY-A (LN) 
(IF (EQUAL LN 'YPC) 0 
(IF (EQUAL LN 'YPCN) 1 
(IF (EQUAL LN 'YPR) 0 
(IF (EQUAL LN 'YIN) 1 
(IF (EQUAL LN 'YI) 2 
(IF (EQUAL LN 'YE) 3 
(IF (EQUAL LN 'YOUT) 0 
0))))))))

(DEFN SY-A (LN X) 
(IF (EQUAL LN 'YPC) (IF (EMPTY X) (E) (I 0 (SY-A 'YPCN (P X)))) 
(IF (EQUAL LN 'YPCN) (S-INC (SY-A 'YPC X)) 
(IF (EQUAL LN 'YPR) (IF (EMPTY X) (E) (I 'IPR (SY-A 'YPR (P X)))) 
(IF (EQUAL LN 'YIN) (S-UMMX (SY-A 'YPC X) (SY-A 'YPR X)) 
(IF (EQUAL LN 'YI) (S-UI (SY-A 'YIN X)) 
(IF (EQUAL LN 'YE) (S-UE (SY-A 'YI X)) 
(IF (EQUAL LN 'YOUT) (IF (EMPTY X) (E) (I 0 (SY-A 'YE (P X)))) 
(SFIX X))))))))
((LEX2 (LIST (COUNT X) (TOPOR-SY-A LN)))) )

;; A2-Begin-SY-A

(PROVE-LEMMA A2-EMPTY-SY-A (REWRITE)
    (EQUAL (EMPTY (SY-A LN X)) (EMPTY X))
    ((DISABLE S-INC S-UMMX S-UI S-UE)))

