; This takes care of the PC loop:
(prove-lemma EQ-PC (rewrite)
(equal (sy-b 'Ypc x)
       (sy-a 'Ypc x))
((induct (induct-P x)))
)

; This takes care of the PR loop:
(prove-lemma EQ-PR (rewrite)
(equal (sy-b 'Ypr x)
       (sy-a 'Ypr x))
((induct (induct-P x)))
)

; EQ-A-B-exp: explicit cork

(prove-lemma EQ-A-B-exp ()
(equal (sy-b 'Ye x)
       (if (empty x) (e) (I (UE 0)
       (if (empty (P x)) (e) (I (UE (UI 0))
				(sy-a 'Ye (P (P x)))
				))))
       )
((expand (sy-b 'Ye x)
	 (SY-B 'YI2 X)
	 (SY-B 'YI (P X))
	 (SY-A 'YE (P (P X)))
	 (SY-B 'Yin2 (P X))
	 (SY-A 'YI (P (P X)))
	 (SY-B 'Yin (P (P X)))
	 (SY-A 'Yin (P (P X)))
	 )
 )
)

; w/ implicit cork:

(prove-lemma EQ-A-B-imp ()
(implies (not (empty (P x)))
	 (equal (B (B (sy-b 'Ye x)))
		(sy-a 'Ye (P (P x)))
		))
((expand (sy-b 'Ye x)
	 (SY-B 'YI2 X)
	 (SY-B 'YI (P X))
	 (SY-A 'YE (P (P X)))
	 (SY-B 'Yin2 (P X))
	 (SY-A 'YI (P (P X)))
	 (SY-B 'Yin (P (P X)))
	 (SY-A 'Yin (P (P X)))
	 )
 )
)
